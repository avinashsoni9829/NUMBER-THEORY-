#include<bits/stdc++.h>
#define ll long long
using namespace std;

ll int expo(ll int a,ll int b)
{
	ll int ans=1;
	
	while(b>0)
	{
		if( b&1)
		
		ans*=a;
		
		a*=a;
		
		b>>=1;

 	}
 	
 	return ans;
 	
 	
}


 
int main()
{
	while(1)
	{
	
    ll int a,b;
	cin>>a>>b;
	
	cout<<expo(a,b)<<endl;
		
   }
   
   
}
