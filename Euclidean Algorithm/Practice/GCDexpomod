//GCD(a^n,c)
#include<bits/stdc++>
using namespace std;
#define ll long long int

ll expo(ll a,ll b,ll m)
{
	ll ans =1;
	a=(a)%m;
	
	while(b>0)
	{
		if(b&1)
		{
			ans=(ans*a)%m;
		}
		a=(a*a)%m;
		b>>=1;
		
		
	}
	
	return ans;
}


ll gcdp(ll int a,ll int n,ll int c)
{
	if(a%c==0)
	return c;
	
	ll modx=expo(a,n,c);
	
	return __gcd(modx,c);
	
	
}

int main()
{
	ll a,n,c;
	cin>>a>>n>>c;
	cout<<gcdp(a,n,c)<<endl;
	
}
